/*************/
/* Terminals */
/*************/
terminal INT returns ecore::EInt: '-'?('0'..'9')+;

terminal STRING	: 
	'"' ( '\\' ('b'|'t'|'n'|'f'|'r'|'"'|"'"|'\\') | !('\\'|'"') )* '"' |
	"'" ( '\\' ('b'|'t'|'n'|'f'|'r'|'"'|"'"|'\\') | !('\\'|"'") )* "'"; 
			
terminal FREE_FORM_COMMENT:
	('a'..'z'|'A'..'Z'|'0'..'9'|'('|')'|'['|']'|',')+;
	
terminal ML_COMMENT:
	(SL_COMMENT | (' '* '\t'* ('\r'|'\n'))+) !('#'|'\r'|'\n') !('\r'|'\n')*;

terminal SL_COMMENT: 
	('-' !('0'..'9') !('\n'|'\r')* ('\r'|'\n')?) |
	('//' !('\n'|'\r')* ('\r'|'\n')?);
	
terminal WS:
	(' '|'\t'|'\r'|'\n')+;	
	